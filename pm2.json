{
    "apps": [
        {
            "name": "huobi-server",
            "script": "./dist/src/app.js",
            "cwd": "./",
            "watch": [
                "dist/config",
                "dist/src"
            ],
            "env": {
                "NODE_ENV": "development"
            },
            "env_production" : {
                "NODE_ENV": "production"
            },
            "error_file": "./logs/err/app-err.log",
            "out_file": "./logs/out/app-out.log",
            "log_date_format": "YYYY-MM-DD HH:mm ss"
        }
    ]
}
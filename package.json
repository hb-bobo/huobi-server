    "ioredis": "^4.17.3",
    "json-bigint": "^0.3.0",
    "jsonwebtoken": "^8.4.0",
    "koa": "^2.13.0",
    "koa-body": "^4.1.1",
    "koa-bodyparser": "^4.3.0",
    "koa-convert": "^1.2.0",
    "koa-cors": "0.0.16",
    "koa-json": "^2.0.2",
    "koa-onerror": "^3.1.0",
    "koa-router": "^7.2.1",
    "koa-session": "^5.12.3",
    "koa-static": "^3.0.0",
    "koa2-connect-history-api-fallback": "^0.1.2",
    "lodash": "^4.17.19",
    "log4js": "^5.3.0",
    "module-alias": "^2.2.2",
    "moment": "^2.22.2",
    "mysql": "^2.18.1",
    "node-config-ts": "^3.0.4",
    "nodemailer": "^4.7.0",
    "pako": "^1.0.11",
    "redis": "^3.0.2",
    "reflect-metadata": "^0.1.13",
    "request": "^2.88.0",
    "request-promise": "^4.2.4",
    "socket.io": "^2.3.0",
    "superagent": "^4.1.0",
    "typeorm": "^0.2.24",
    "validator": "^13.0.0",
    "ws": "^7.2.0",
    "xss": "^1.0.3"
  },
  "devDependencies": {

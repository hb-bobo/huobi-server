    "nodemon": "^2.0.3",
    "prettier": "^1.18.2",
    "ts-jest": "^25.4.0",
    "ts-node": "^8.4.1",
    "ttypescript": "^1.5.8",
    "typescript": "^3.6.4"
  }
}

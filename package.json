{
  "name": "huobi-server",
  "version": "0.1.0",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/hb-bobo/huobi-server.git"
  },
  "scripts": {
    "start": "cross-env NODE_ENV=development nodemon --config nodemon.json",
    "start:prod": "cross-env NODE_ENV=production node dist/src/app.js",
    "dev": "npm run start:dev",
    "gulp": "gulp",
    "build": "npm run test && npm run gulp",
    "pm2:prod": "pm2 start pm2.json --env production",
    "deploy:beta": "cross-env NODE_ENV=beta npm run build && node scripts/deploy.js deploy-beta",
    "deploy:prod": "cross-env NODE_ENV=production npm run build && node scripts/deploy.js deploy-master",
    "docker-compose:dev": "docker-compose -f docker-compose.dev.yml up -d --force-recreate",
    "docker-compose:prod": "docker-compose -f docker-compose.yml up -d --force-recreate",
    "eslint": "eslint ./src --ext .ts,.tsx --quiet",
    "tslint": "tsc --project ./tsconfig.json --noEmit",
    "test": "jest --colors --onlyChanged"
  },
  "_moduleAliases": {
    "@root": ".",
    "ROOT": "./src"
  },
  "dependencies": {
    "async-validator": "^3.5.1",
    "config": "^3.3.3",
    "cross-env": "^5.2.0",
    "dayjs": "^1.10.4",
    "debug": "~2.6.8",
    "got": "^11.8.1",
    "ioredis": "^4.22.0",
    "jsonwebtoken": "^8.4.0",
    "koa": "^2.13.1",
    "koa-body": "^4.2.0",
    "koa-bodyparser": "^4.3.0",
    "koa-convert": "^1.2.0",
    "koa-cors": "0.0.16",
    "koa-json": "^2.0.2",
    "koa-onerror": "^3.1.0",
    "koa-router": "^7.2.1",
    "koa-session": "^5.12.3",
    "koa-static": "^3.0.0",
    "koa2-connect-history-api-fallback": "^0.1.3",
    "lodash": "^4.17.21",
    "log4js": "^6.3.0",
    "module-alias": "^2.2.2",
    "mysql": "^2.18.1",
    "node-huobi-sdk": "^1.0.25",
    "nodemailer": "^4.7.0",
    "redis": "^3.1.1",
    "reflect-metadata": "^0.1.13",
    "socket.io": "^2.4.1",
    "typeorm": "^0.2.31",
    "validator": "^13.5.2",
    "xlsx": "^0.16.9",
    "xss": "^1.0.8"
  },
  "devDependencies": {
    "@types/config": "0.0.35",
    "@types/ioredis": "^4.19.4",
    "@types/jest": "^25.2.3",
    "@types/jsonwebtoken": "^8.3.5",
    "@types/koa": "^2.13.0",
    "@types/koa-bodyparser": "^4.2.2",
    "@types/koa-cors": "0.0.0",
    "@types/koa-json": "^2.0.18",
    "@types/koa-logger": "^3.1.1",
    "@types/koa-router": "^7.4.1",
    "@types/koa-session": "^5.10.3",
    "@types/koa-static": "^4.0.1",
    "@types/lodash": "^4.14.168",
    "@types/node": "^12.20.4",
    "@types/socket.io": "^2.1.13",
    "@types/validator": "^13.1.3",
    "@types/ws": "^6.0.3",
    "@typescript-eslint/eslint-plugin": "^4.15.1",
    "@typescript-eslint/parser": "^4.15.1",
    "babel-jest": "^25.5.1",
    "eslint": "^7.20.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-jest": "^23.20.0",
    "eslint-plugin-prettier": "^3.3.1",
    "gh-pages": "^2.2.0",
    "gulp": "^4.0.2",
    "gulp-ts-alias": "^1.3.0",
    "gulp-typescript": "^6.0.0-alpha.1",
    "jest": "^25.5.2",
    "merge-stream": "^2.0.0",
    "nodemon": "^2.0.7",
    "prettier": "^1.18.2",
    "ts-jest": "^25.4.0",
    "ts-node": "^8.10.2",
    "typescript": "^4.1.5"
  }
}
